{
  "name": "crwn-designs-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.x",
    "npm": "6.14.5"
  },
  "description": "Server for crwn-designs.now.sh",
  "main": "server.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "build": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aman-atg/ecommerce-site.git"
  },
  "keywords": [
    "server"
  ],
  "author": "Aman Ansari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aman-atg/ecommerce-site/issues"
  },
  "homepage": "https://github.com/aman-atg/ecommerce-site#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "stripe": "^8.52.0"
  }
}
